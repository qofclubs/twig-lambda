{
    "name": "mattimatti/twig-lambda",
    "type": "library",
    "description": "Lambda expressions for Twig and filters that make use of them",
    "keywords": [
        "lambda",
        "function",
        "unique",
        "functional",
        "filter",
        "map",
        "group",
        "sort"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Polac",
            "email": "damian.polac.111@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:mattimatti/dictionary"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "twig/twig": "^1.0 || ^2.0",
        "dpolac/dictionary": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "DPolac\\TwigLambda\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
